<h1>New Publication</h1>

<% form_for @publication, :url => { :action => "create" } do |f| %>
  <% f.error_messages %>

  <%= render :partial  => "#{@publication.current_step}_bibtex_step", :locals => { :f => f }  %>

  <p><%= f.submit "Submit" %></p>
  <p><%= f.submit "Back", :name => "back_button" unless @publication.first_step? %></p>
  
<% end %>

