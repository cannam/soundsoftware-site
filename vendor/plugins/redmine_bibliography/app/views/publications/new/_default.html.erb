<% form_for @publication, :url => { :project_id  => @project, :action => :create }, :builder => TabularFormBuilder do |f| -%>
  <div class="box tabular settings">
    <fieldset>
      <%= render :partial => 'form', :locals => { :f => f }  %>

      <div style="clear:both"></div>
      <%= f.submit %>
    </fieldset>
  </div>
<%- end -%>