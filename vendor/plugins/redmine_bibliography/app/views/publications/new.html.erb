<h1>New Publication</h1>

<% form_for(:publication) do |f| %>
  <% f.error_messages %>
  
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>


  <p>
  
    <%=label_tag :bibtex_entry %>
    <%=text_area_tag :bibtex_entry%>
  
  </p>

<%= submit_tag l(:button_submit) %>

<% end %>



